#main
  .header
    transform-style preserve-3d
    position fixed
    left 0px
    top 0px
    z-index 10
    width 100%
    height 130px
    .head
      width 100%
      height 40px
      background #f1f1f1
      transition transform 1s
      &.on
        animation on 1s
      .content
        width 1200px
        height 100%
        margin 0 auto
        .left
          float left
          width 175px
          height 100%
          display flex
          justify-content center
          align-items center
          span
            &:nth-child(2)
              color #000
              font-weight bold
          img
            display inline-block
            vertical-align middle
        .nav
          display flex
          float right
          height 100%
          ul
            height 100%
            display flex
            li
              float left
              height 100%
              padding 0 15px
              a
                display flex
                align-items center
                justify-content center
                font-weight bold
                color #000000
                width 100%
                height 100%
                &.active
                  color red

      .nav-search
        width 1200px
        height 90px
        margin 0 auto
        display flex
        align-items center
        div
          float left
        .logo
          width 280px
          height 100%
          display flex
          justify-content center
          align-items center
      .search
        width 600px
        height 38px
        margin-left 100px
        border 2px solid red
        border-radius 5px
        display flex
        align-items center
        position relative
        .iconfont
          color:#999
          position absolute
          left 23px
          top 12px
          font-weight bold
        input
          width 70%
          height 30px
          outline none
          margin-left 60px
          padding 0 30px 0 0
          font-size 15px
        button
          width 20%
          height 100%
          background red
          outline none
          border 1px solid red
          color white
          font-weight bold
      .code
        width 90px
        height 100%
        position relative
        left 125px
        top 0px
        text-align center
        .toggle
          width 0
          height 0
          border-top 8px solid transparent
          border-left 8px solid transparent
          border-right 8px solid transparent
          border-bottom 8px solid white
          position absolute
          left 35px
          top -15px
        img
          width 100%
  .htit
    margin-top 130px
    width 100%
    background: #fff;
    .htit-center
      height: 140px;
      line-height: 140px;
      width: 1200px;
      margin: 0 auto;
      .left
        float: left;
        margin-left: 20px;
        margin-top: 10px;
        height: 120px;
        b
          font-size: 36px;
          font-weight: 700;
        i
          margin-left: 10px;
          color: #929292;
          list-style: none;
          font-style: normal;
          padding: 0;
          em
            color: #121212;
      .right
        float: right;
        height: 120px;
        margin-top: 15px;
        ul
          li
            display: inline-block;
            width: 100px;
            text-align: center;
            line-height: 22px;
            margin-top: 52px;
            i
              list-style: none;
              font-style: normal;
              margin: 0;
              padding: 0;
              display: block;
              color: #e84b47;
            em
              position: relative;
              bottom: 12px;
              left: 20px;
              color: #e5e5e5;
  .navtit
    width: 100%;
    height: 40px;
    line-height: 40px;
    background: #323434;
    clear: both;
    ul
      width: 1200px;
      margin: 0 auto;
      height 100%
      .slibingsclor
        background: #424242;
      li
        width: 140px;
        height: 40px;
        color: #fff;
        text-align: center;
        display: inline-block;
        a
          color: #fff;

  .service-center
    margin-top: 22px;
    width: 1200px;
    .centerleft
      float: left;
      margin-bottom: 50px;
      width: 960px;
      background: #fff;
      h3
        width: 95%;
        border: none;
        border-bottom: 1px solid #e5e5e5;
        margin-left: 3%;
        height: 60px;
        line-height: 60px;
        font-weight: normal;
        font-size: 14px;
        color: #e5e5e5;
        background: #fff;
        overflow: hidden;
        i
          color: #121212;
          margin: 0;
          border-bottom: 4px solid #fb474f;
          font-size: 16px;
          padding-bottom: 16px;
          cursor: pointer;
      .boxtab
        a
          .ctop
            border: none;
            border-bottom: 1px solid #e5e5e5;
            cursor: pointer;
            height: 200px;
            .pic
              border: 1px solid #e5e5e5;
              float: left;
              margin: 25px;
              vertical-align: middle;
              width 150px
              height 150px
            .spm
              float: left;
              position: relative;
              top: 25px;
              width: 64%;
              span
                &:nth-child(1)
                  overflow: hidden;
                  -webkit-line-clamp: 1;
                  white-space: nowrap;
                  text-overflow: ellipsis;
                  width: 99%;
                  display: block;
                  line-height: 42px;
                  height: 42px;
                  font-size: 18px;
                  color: #1d1515;
                &:nth-child(2)
                  color: #929292;
                  overflow: hidden;
                  -webkit-line-clamp: 1;
                  white-space: nowrap;
                  text-overflow: ellipsis;
                  width: 99%;
                  display: block;
                  line-height: 42px;
                  height: 42px;
                &:nth-child(3)
                  overflow: hidden;
                  -webkit-line-clamp: 1;
                  white-space: nowrap;
                  text-overflow: ellipsis;
                  width: 99%;
                  margin-top: 20px;
                  display: block;
                  line-height: 42px;
                  height: 42px;
              .price
                font-size: 30px;
                color: #fc545e;
                font-weight: 700;
                position relative
              i
                font-size: 14px;
                font-style: normal;
                position absolute
                left 60px
                top 0px
            .spf
              color: #929292;
              float: right;
              margin: 150px 60px 0 0;
      .intro
        width: 99%;
        height: 60px;
        line-height: 60px;
        border: 1px solid #eaeaea;
        font-weight: normal;
        font-size: 14px;
        color: #e5e5e5;
        background: #fff;
        margin: 0;
        overflow: hidden;
        border-bottom: 1px solid #e5e5e5;
        margin-left: 3%;
      .company
        height: 40px;
        line-height: 40px;
        font-size: 18px;
        margin: 10px 25px;
        img
          margin-left: 10px;
          vertical-align: middle;
      .profile
        line-height: 28px;
        margin: 10px 25px 25px;
        font-size: 16px;
      .pinglun
        width: 95%;
        border: none;
        border-bottom: 1px solid #e5e5e5;
        margin-left: 3%;
        height: 60px;
        line-height: 60px;
        font-weight: normal;
        font-size: 14px;
        color: #e5e5e5;
        background: #fff;
        overflow: hidden;
        .paihang
          .activity
            color: #121212;
            margin: 0;
            border-bottom: 4px solid #fb474f;
          em
            color: #121212;
            margin-left: 15px;
      .commentDiv
        .conmentss
          width: 96%;
          border-bottom: 1px solid #e5e5e5;
          margin: 0 3%;
          padding-top: 20px;
          .touxiang
            border-radius: 6px;
            display: inline-block;
            vertical-align: top;
            width: 40px;
            height: 40px;
          ul
            display: inline-block;
            margin-left: 20px;
            width: 92%;
            li
              border: none;
              width: auto;
              height: auto;
              display: block;
              float: none;
              padding: 0;
              line-height: 28px;
              margin: 0;
              .star
                width: 70px;
                margin-left: 0;
                img
                  vertical-align: middle;
                  width: 10px;
              em
                margin-left: 10px;
                font-style: normal;
            .commentdetail
              line-height: 26px;
              font-size: 16px;
            .commentposition
              height: 40px;
              color: #898989;
              img
                margin-right: 10px;
                vertical-align: middle;
