@import 'mixins.styl'.container  color #333  .header    position relative    width 100%    height 71px    box-sizing border-box    padding 2.5px 0    background rgba(255, 255, 255, .96)    border-bottom 1px solid #eaeaea    &.on      position fixed      z-index 1111      transform-origin center top      box-shadow 0 3px 5px #7e8c8d      animation move 1s    nav      width 1200px      height 100%      margin 0 auto      padding 0 15px      &.clearFix        &::after          content ''          display block          clear both      .navLogo        float left        height 100%        width 280px        .logo          display inline-block          height 100%          img            margin-top 8px      .navBar        float right        height 100%        ul          margin-top 10px          height 42px          color  #121212          font-size 18px          li            float left            height 100%            &:first-child              margin-right 50px              img                vertical-align middle              p                display inline-block                margin-right 5px            &:last-child              margin-right 10px              a                background #e94840                display inline-block                height 70%                padding 4px 6px                margin-top 7px                margin-left 15px                box-sizing border-box                color #fff!important                border-radius 6px                line-height 1            &.active              a                color #e94840            a              display inline-block              height 100%              padding 10px 15px              color #121212              box-sizing border-box              line-height 1              >span                font-weight bold                &:not(.login):hover                  transition color 0.5s                  color #e94840  .carousel    width 100%    height 420px    font-weight bold    overflow hidden    color white    position relative    .carBanner      width 100%      height 100%      .lunbo-container        position relative;        width 100%;        height 100%;        overflow hidden        .bd          position relative          height 100%          z-index 0          li            width 100%            height 100%            &:nth-of-type(1)              a                background url("../images/banner1.jpg") no-repeat                background-position center center            &:nth-of-type(2)              a                background url("../images/banner2.jpg") no-repeat                background-position center center            &:nth-of-type(3)              a                background url("../images/banner3.jpg") no-repeat                background-position center center          a            display block            width 100%            height 100%        .hd          position absolute          bottom 40px          left 50%          margin-left -55px          padding 2px 5px          height 10px          z-index 10          .bullet            position relative            float left            width 10px            height 10px            background #757881            margin 0 5px            border-radius 50%            cursor pointer            z-index 2            &.on              background #ffffff          .bullet-bg            position absolute            left 0            top 0            right 0            bottom 0            border-radius 6px            z-index 1    .navBanner      position absolute      top 0      left 10%      height 100%      .wrapList        height 100%        width 180px        >li          height 42px          width 100%          background #757881          >.classList            position absolute            left 0            right 0            height 42px            color white            line-height 42px            padding-left 45px            .arrow              opacity 0              position absolute              right 0              top 50%              transform translateY(-50%)              content ''              display block              border 5px solid              border-color #757881 #fff #757881 #757881            span              font-weight bold              font-size 18px            >i              margin-left 20px            &:hover              background #60646D              .arrow                opacity 11                border-color #60646D #fff #60646D #60646D                transition opacity 1s              +.contentWrap                display block              >i                opacity 0                transition opacity 0.5          >.contentWrap            display none            position absolute            top 0            bottom 0            left 180px            width 160px            background #fff            &:hover              display block            ul              height 100%              width 100%              li                position relative                height 42px                width 100%                a                  display inline-block                  position absolute                  left 0                  right 0                  height 100%                  line-height 42px                  text-align center                  &:hover                    span                      color #e94840                  span                    color #333                    font-weight bold                    font-size 18px  .details    width 100%    padding-bottom 20px    background #fff    header      width 1200px      margin 0 auto      padding-top 50px      text-align center      .title        width 100%        text-align center        color #3d3838        >span          font-weight 900          font-size 28px      .subTitle        position relative        width 100%        font-size 22px        font-family '幼圆'        margin-top 15px        ul          display inline-block          li            float left            margin-right 10px            &:hover              a                transition color 1s                color #e94840        .getMore          position absolute          right 0          top -5px          display inline          cursor pointer          span            vertical-align middle          .iconfont            line-height 40px          &:hover            cursor pointer            color #e94840    .middleContent      width 1200px      margin 0 auto      padding-top 20px      .wrap        margin-top 10px        width 100%        ul          li            float left            width 208px            height 345px            background #fff            border 1px solid #eaeaea            padding 10px            margin 0 5px            cursor pointer            &:hover              box-shadow 0 3px 10px #7e8c8d              transition box-shadow 0.5s            img              width 210px              height 210px            .middleText              width 100%              .title                padding 10px 0                color #333333                overflow hidden                text-overflow ellipsis                white-space nowrap                span                  font-size 16px                  font-weight 900              .titleContent                width 100%                overflow hidden                text-overflow ellipsis                padding 5px 2px                margin-bottom 10px                display -webkit-box                -webkit-line-clamp 2                -webkit-box-orient vertical                font-size 13px                line-height 18px                color #929292            .botPrice              width 100%              .oneHourPrice                float left                color #e94840                span                  font-weight 900                  .money                    font-size 22px              .seeMore                float right                background #e94840                border-radius 5px                color #fff                font-size 14px                text-align center                width 76px                height 24px                line-height 24px    .bottomBom      width 1200px      margin 20px auto 0      padding 10px      border 1px solid #eaeaea      a        font-size 16px        margin-right 7px        color #3d3838        line-height 32px        cursor pointer        &:hover          color #e94840          transition color 1.5s  .ServiceContent    width 1200px    margin auto    h2      line-height 85px      font-weight 500      font-size 18px    .shopList      width 970px      float left      margin-bottom 50px      .item        width 232px        height 300px        border 1px solid #eaeaea        padding 10px        float left        margin-right 10px        margin-bottom 10px        background #fff        cursor pointer        box-sizing border-box        &:hover          box-shadow 0px 5px 10px #7e8c8d        img          display inline-block          width 100%        .middle          margin-top 15px          white-space nowrap          overflow hidden          text-overflow ellipsis          height 26px          line-height 26px          font-size 16px          span            margin-left 3px        .bottom          margin-top 15px          color #929292          font-size 12px          .left            float left          .right            float right    .advertising      float: right      width 230px      img        display block        width 100%  >.img    width 100%    height 300px    background url("../images/information.jpg") center center no-repeat  .infoList    >div      width 100%      height 300px      background: pink;      .inner        width 1200px        margin 0 auto    .infoLeft      background #fff      img        margin 60px      .content        float right        margin-right 150px        line-height 60px        padding-top 100px        h2          font-size 32px          font-weight 900        h3          font-size 22px          font-weight 600    .infoRight      background #f8f8f8      .bigPic        margin 60px 60px 60px 0px      img        float right        margin 60px      .content        float left        margin-right 150px        line-height 60px        padding-top 100px        h2          font-size 32px          font-weight 900        h3          font-size 22px          font-weight 600  footer    width 100%    .promise      margin-top 100px      text-align center      .server        font-size 28px        font-weight 900        .special          color #e94840          line-height 1      .promiseText        font-size 25px        font-weight 600        line-height 2.5      .promiseLogo        display flex        justify-content center        >div          font-size 22px          font-weight bold          margin-right 25px          .iconfont            font-size 22px            color green    .city      width 1200px      margin 0 auto      padding-top 30px      height 150px      .title        height 150px        width 100px        display inline-block        float left        font-weight 800        font-size 22px      .hotCitys        width 1070px        height 150px        display inline-block        float left        margin-left 20px        line-height 30px        a          font-size 16px          text-align center          display inline-block          width 9%          color #898989          &:hover            color #e94840    .teamwork      width 1200px      margin 0 auto      margin-top 20px      .title        color #333        font-size 22px        font-weight 800        margin-bottom 20px      .img        ul          li            width 10%            height 60px            text-align center            line-height 55px            float left            margin-right 10px            img              width 100%              height 100%    .banquan      width 1200px      margin 0 auto      font-size 14px      color #7e8c8d@keyframes move{  0%{    opacity 1    transform rotateX(0)  }  10%{    opacity 0    transform rotateX(90deg)  }  100%{    opacity 1    transform rotateX(0)  }}.clearFix  zoom 1  &::after    content ''    display block    clear both